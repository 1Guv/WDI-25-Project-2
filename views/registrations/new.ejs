<h1>REGISTER</h1>
<div class="form">
  <form method="POST" action="/register" enctype="multipart/form-data">
    <input type="hidden" name="lat">
    <input type="hidden" name="lng">
    <div>
      <label class="form-group" for="fname">First Name</label>
      <input class="form-control" type="text" name="fname" id="fname">
    </div>
    <div>
      <label class="form-group" for="sname">Second Name</label>

      <input class="form-control" type="text" name="sname" id="sname">
    </div>
    <div>
      <label class="form-group" for="addressLine1">Address - Line 1:</label>

      <input class="form-control" type="text" name="addressLine1" id="addressLine1">
    </div>
    <div>
      <label class="form-group" for="addressLine2">Address - Line 2:</label>

      <input class="form-control" type="text" name="addressLine2" id="addressLine2">
    </div>
    <div>
      <label class="form-group" for="postcode">Postcode:</label>
      <br>
      <input class="autocomplete" type="text" name="postcode" id="postcode">
      <input type="hidden" name="lat">
      <input type="hidden" name="lng">
    </div>

    <div>
      <label class="form-group" for="image">Profile Pic:</label>

      <input class="" type="file" name="image" id="image">
    </div>
    <div>
      <label class="form-group" for="username">Username:</label>

      <input class="form-control" type="text" name="username" id="username">
    </div>
    <div>
      <label class="form-group" for="email">Email</label>

      <input class="form-control" type="email" name="email" id="email">
    </div>
    <div>
      <label class="form-group" for="password">Password</label>

      <input class="form-control" type="password" name="password" id="password">
    </div>
    <div>
      <label class="form-group" for="passwordConfirmation">Password Confirmation</label>

      <input class="form-control" type="password" name="passwordConfirmation" id="passwordConfirmation">
    </div>

    <button class="rbtn">Register</button>
  </form>


</div>
