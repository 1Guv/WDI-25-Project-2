<h1>PROFILE PAGE</h1>
<h1><%= user.username %></h1>
<h1><%= user.email %></h1>
<h1><%= user.fname %></h1>
<h1><%= user.sname %></h1>
<h1><%= user.addressLine1 %></h1>
<h1><%= user.addressLine2 %></h1>
<h1><%= user.postcode %></h1>
<img src="<%= user.imageSRC %>" alt="<%= user.username %>" height="100" width="200">

<div class="">
  <a class="rbtn" href="#">EDIT</a>
  <a class="rbtn" href="#">DELETE MY ACCOUNT</a>
  <a class="rbtn" href="/supercars/new">ADD MY SUPERCAR</a>
  <a class="rbtn" href="/supercars/">SEARCH FOR SUPERCARS</a>
</div>

<p></p>
    <div class="map1" data-user="<%= JSON.stringify(user) %>"></div>

<h2>My Super Cars</h2>
<p></p>
<div class="">
  <% cars.forEach((car) => { %>
    <h1><%= car.make %></h1>
    <h1><%= car.model %></h1>
    <h1><%= car.carpic %></h1>
    <img src="<%= car.imageSRC %>" alt="<%= car.make %>" height="100" width="200">
  <% }) %>
</div>
